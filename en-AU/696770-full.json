      "tabs": {
        "overviewTab": {
          "content": "<div><overview-container/>\n\n  <!-- start video banner -->\n  <pc-video-banner\n    data-translate\n    video-title=\"Youtube Video\"\n    video-link=\"https://www.youtube-nocookie.com/embed/473UuvBYfXI\"\n    bg-image=\"https://res.garmin.com/en/products/010-TTRNG-AP/g/tacx-video_v01-eb7a7a62-fe64-4dae-a7d7-39009e62cc61.jpg\"\n    mobile-bg-image=\"https://res.garmin.com/en/products/010-TTRNG-AP/g/696770-video-M.jpg\"\n    img-alt=\"video\"\n    not-youtube=\"false\"\n    theme=\"dark\"\n  />\n  <!-- end video banner -->\n  <!-- start overview intro -->\n  <pc-overview-intro\n    data-translate\n    title=\"GET MORE FROM YOUR RIDE\"\n    title-heading=\"h2\"\n    title-align=\"center\"\n    description=\"Pair your Tacx<sup>®</sup> smart trainer with the Tacx Training app on your compatible smartphone, tablet or desktop.\"\n    description-size=\"large\"\n    description-align=\"center\"\n    theme=\"light\"\n  >\n    <pc-feature-card\n      data-translate\n      title=\"\"\n      description=\"From phone to tablet to desktop, one app gets you the best virtual cycling experience.\"\n      image=\"https://res.garmin.com/gdc/Product_Pages/ICONS/SYNC.svg\"\n      img-alt=\"\"\n      img-width=\"50px\"\n    />\n    <pc-feature-card\n      data-translate\n      title=\"\"\n      description=\"Sync in a snap. Your indoor training data automatically loads to the Garmin Connect™ app.\"\n      image=\"https://res.garmin.com/gdc/Product_Pages/ICONS/Garmin_Connect.svg\"\n      img-alt=\"\"\n      img-width=\"50px\"\n    />\n    <pc-feature-card\n      data-translate\n      title=\"\"\n      description=\"Ride famous and exciting courses without ever leaving your home<sup>1</sup>.\"\n      image=\"https://res.garmin.com/gdc/Product_Pages/ICONS/Camera.svg\"\n      img-alt=\"\"\n      img-width=\"50px\"\n    />\n    <pc-feature-card\n      data-translate\n      title=\"\"\n      description=\"Plan group rides to connect with friends from all over the world<sup>1</sup>.\"\n      image=\"https://static.garmincdn.com/gdc/Product_Pages/ICONS/Group-Ride.svg\"\n      img-alt=\"\"\n      img-width=\"50px\"\n    />\n    <pc-feature-card\n      data-translate\n      title=\"\"\n      description=\"Speed, power, cadence and more. Track them and see them on-screen.\"\n      image=\"https://res.garmin.com/gdc/Product_Pages/ICONS/Measure_Data.svg\"\n      img-alt=\"\"\n      img-width=\"50px\"\n    />\n    <pc-feature-card\n      data-translate\n      title=\"\"\n      description=\"Stay on track every day — your scheduled training plan workouts from Garmin Connect now appear in the Tacx Training app.\"\n      image=\"https://res.garmin.com/gdc/Product_Pages/ICONS/Animated_Workouts.svg\"\n      img-alt=\"\"\n      img-width=\"50px\"\n    />\n  </pc-overview-intro>\n  <!-- end overview intro -->\n</div><div>  <!-- start life style -->\n  <pc-life-style\n    data-translate\n    text=\"Indoor training — the Tacx® way.\"\n    text-size=\"2.6\"\n    text-align=\"left\"\n    text-position=\"middle\"\n    mobile-text-size=\"2.4\"\n    mobile-text-align=\"center\"\n    mobile-text-position=\"top\"\n    button-text=\"\"\n    link=\"\"\n        bg-image=\"https://res.garmin.com/it-production/image/upload/v1666806853/Product_Images/en/products/010-TTRNG-AP/g/55563-D-1.jpg\"\n        mobile-bg-image=\"https://res.garmin.com/it-production/image/upload/v1666806853/Product_Images/en/products/010-TTRNG-AP/g/55563-M-1.jpg\"\n    img-alt=\"\"\n    img-width=\"\"\n    theme=\"dark\"\n  />\n  <!-- end life style -->\n  <!-- start product feature section -->\n  <pc-layout-container>\n    <pc-column-layout\n      columns=\"3\"\n    >\n      <pc-feature-card\n        data-translate\n        title=\"HIGH-QUALITY TRAINING VIDEOS\"\n        description=\"Explore the world with a large collection of training videos<sup>1</sup>, including everything from famous spring classics to the great climbs of the Alps. Plus, train with WorldTour pros on training rides.\"\n        image=\"https://res.garmin.com/it-production/image/upload/v1667576491/Product_Images/en/products/010-TTRNG-AP/g/V1-55563-1.jpg\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n      <pc-feature-card\n        data-translate\n        title=\"VIRTUAL GROUP RIDES\"\n        description=\"Invite your friends to ride, train and race with you in the Tacx Training app<sup>1</sup>. See other riders in your group in real time, plus compare stats and results on the leaderboard once you’ve finished.\"\n        image=\"https://res.garmin.com/en/products/010-TTRNG-AP/g/60686-Virtual-Group-Ride.jpg\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n      <pc-feature-card\n        data-translate\n        title=\"PERFORMANCE DATA\"\n        description=\"While you ride, your speed, power, cadence and <a href='https://www.garmin.com/en-AU/c/sports-fitness/cycling-biking-accessories/?productFilterAccessoryGroupHRM=true'>heart rate<sup>2</sup></a> are displayed on-screen. Your stats automatically load to the <a href='https://www.garmin.com/en-AU/p/125677'>Garmin Connect</a> app for more detailed analysis after your workout.\"\n        image=\"https://res.garmin.com/it-production/image/upload/v1667576491/Product_Images/en/products/010-TTRNG-AP/g/V1-55563-3.jpg\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n    </pc-column-layout>\n    <pc-column-layout\n      columns=\"3\"\n    >\n      <pc-feature-card\n        data-translate\n        title=\"LIVE OPPONENTS\"\n        description=\"See other riders who are riding the same course as you — in real time<sup>1</sup> — and let your competitive side out to chase or pull away from them.\"\n        image=\"https://res.garmin.com/it-production/image/upload/v1667576491/Product_Images/en/products/010-TTRNG-AP/g/V1-55563-4.jpg\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n      <pc-feature-card\n        data-translate\n        title=\"FREE STRUCTURED WORKOUTS\"\n        description=\"Complete structured workouts based on target slope, power or FTP. And stay on track with your training with automatically adjusted power targets during workouts in ERG mode.\"\n        image=\"https://res.garmin.com/en/products/010-TTRNG-AP/g/60686-Free-Structured-Workouts.jpg\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n      <pc-feature-card\n        data-translate\n        title=\"VIDEO PLAYLISTS\"\n        description=\"Select one of our featured playlist<sup>1</sup>, and stop and continue your courses at any time.\"\n        image=\"https://res.garmin.com/it-production/image/upload/v1667576491/Product_Images/en/products/010-TTRNG-AP/g/V1-55563-6.jpg\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n    </pc-column-layout>\n    <pc-column-layout\n      columns=\"3\"\n    >\n      <pc-feature-card\n        data-translate\n        title=\"GPS MAP RIDES\"\n        description=\"With 3D map view<sup>1</sup>, there are no boundaries on where you can train. Upload a GPX file to turn your favorite courses into a GPS ride and make the world your virtual playground.\"\n        image=\"https://res.garmin.com/it-production/image/upload/v1667576491/Product_Images/en/products/010-TTRNG-AP/g/V1-55563-2.jpg\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n      <pc-feature-card\n        data-translate\n        title=\"DIG INTO YOUR DATA\"\n        description=\"All the data you rely on is in the Garmin Connect app. See maps and stats, and analyse your progress after your ride.\"\n        image=\"https://res.garmin.com/it-production/image/upload/v1667576492/Product_Images/en/products/010-TTRNG-AP/g/V1-55563-7.jpg\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n      <pc-feature-card\n        data-translate\n        title=\"TACX CHALLENGES\"\n        description=\"Harness your competitive streak by completing a full playlist of rides in a set time to earn Garmin Connect badges.\"\n        image=\"https://res.garmin.com/en/products/010-TTRNG-AP/g/60686-Tacx-Challenges.jpg\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n    </pc-column-layout>\n    <pc-column-layout\n      columns=\"3\"\n    >\n      <pc-feature-card\n        data-translate\n        title=\"IN-APP ONBOARDING\"\n        description=\"For the <a href='https://www.garmin.com/en-AU/p/885302'>Tacx NEO 3M</a> smart trainer, you can walk through each step of the setup process with helpful videos from the app on your compatible mobile device — so you can start riding quicker than ever before.\"\n        image=\"https://res.garmin.com/en/products/010-TTRNG-AP/g/60686-IN-APP-ONBOARDING.jpg\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n      <pc-feature-card />\n\n      <pc-feature-card />\n\n    </pc-column-layout>\n  </pc-layout-container>\n  <!-- end product feature section -->\n  <!-- start life style -->\n  <pc-life-style\n    data-translate\n    text=\"Access the very best of the Tacx Training app.\"\n    text-size=\"2.6\"\n    text-align=\"right\"\n    text-position=\"middle\"\n    mobile-text-size=\"2.4\"\n    mobile-text-align=\"center\"\n    mobile-text-position=\"bottom\"\n    button-text=\"\"\n    link=\"\"\n    bg-image=\"https://res.garmin.com/it-production/image/upload/v1666806854/Product_Images/en/products/010-TTRNG-AP/g/55563-D-2.jpg\"\n    mobile-bg-image=\"https://res.garmin.com/it-production/image/upload/v1666806854/Product_Images/en/products/010-TTRNG-AP/g/55563-M-2.jpg\"\n    img-alt=\"\"\n    img-width=\"\"\n    theme=\"dark\"\n  />\n  <!-- end life style -->\n  <!-- start table -->\n  <pc-layout-container>\n    <pc-copy\n      title=\"\"\n      description=\"While some features are free, we offer more exclusive monthly and annual plans. Enjoy 12 months for the price of 10 with the annual subscription plan.\"\n    />\n   <pc-table>\n      <pc-table-row>\n          <pc-table-cell text=\"\" align=\"left\" data-translate />\n          <pc-table-cell text=\"<b>FREE FEATURES</b>\" align=\"center\" data-translate />\n          <pc-table-cell text=\"<b>PREMIUM FEATURES</b>\" align=\"center\" data-translate />\n          <pc-table-cell text=\"<b>PREMIUM HD FEATURES</b>\" align=\"center\" data-translate />\n      </pc-table-row>\n      <pc-table-row>\n          <pc-table-cell text=\"Structured workouts\" align=\"left\" data-translate />\n          <pc-table-cell checkmark=\"true\" data-translate />\n          <pc-table-cell checkmark=\"true\" data-translate />\n          <pc-table-cell checkmark=\"true\" data-translate />\n      </pc-table-row>\n      <pc-table-row>\n          <pc-table-cell text=\"Analyse data\" align=\"left\" data-translate />\n          <pc-table-cell checkmark=\"true\" data-translate />\n          <pc-table-cell checkmark=\"true\" data-translate />\n          <pc-table-cell checkmark=\"true\" data-translate />\n      </pc-table-row>\n      <pc-table-row>\n          <pc-table-cell text=\"Data export\" align=\"left\" data-translate />\n          <pc-table-cell checkmark=\"true\" data-translate />\n          <pc-table-cell checkmark=\"true\" data-translate />\n          <pc-table-cell checkmark=\"true\" data-translate />\n      </pc-table-row>\n      <pc-table-row>\n          <pc-table-cell text=\"Cross-platform compatibility\" align=\"left\" data-translate />\n          <pc-table-cell checkmark=\"true\" data-translate />\n          <pc-table-cell checkmark=\"true\" data-translate />\n          <pc-table-cell checkmark=\"true\" data-translate />\n      </pc-table-row>\n      <pc-table-row>\n          <pc-table-cell text=\"10-day trial\" align=\"left\" data-translate />\n          <pc-table-cel