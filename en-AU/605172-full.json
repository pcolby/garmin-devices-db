        "inTheBoxTab": {
          "content": "<ul>\n    <li>3 sensors</li>\n    <li>Manual</li>\n    </ul>",
          "title": "In the Box"
        },
        "overviewTab": {
          "content": "<div><overview-container/>\n  <!-- start video banner -->\n  <pc-video-banner\n    data-translate\n    video-title=\"Youtube Video\"\n    video-link=\"https://www.youtube-nocookie.com/embed/BdW3ZPDbevM\"\n    bg-image=\"https://res.garmin.com/en/products/010-01994-00/g/37954-D-VID-126fd0ba-6cd5-4793-8566-1a71ee50f89f.jpg\"\n    mobile-bg-image=\"https://res.garmin.com/en/products/010-01994-00/g/37954-M-VID-531a06b2-83dd-47ef-be3f-b1b6dbdcbe71.jpg\"\n    img-alt=\"video\"\n    not-youtube=\"false\"\n    theme=\"light\"\n  />\n  <!-- end video banner -->  <!-- start overview intro -->\n  <pc-overview-intro\n    data-translate\n    title=\"KNOW YOUR CLUBS\"\n    title-heading=\"h2\"\n    title-align=\"center\"\n    description=\"We’ve got your game with Approach CT10 club tracking sensors. Track every shot on any club — starting with your wedges and putter.\"\n    description-size=\"large\"\n    description-align=\"center\"\n    theme=\"light\"\n  >\n    <pc-feature-card\n      data-translate\n      title=\"\"\n      description=\"You’re in wedge territory. But pitching or gap? You’ll know after attaching a sensor.\"\n      image=\"https://res.garmin.com/gdc/Product_Pages/ICONS/GOLF.svg\"\n      img-alt=\"\"\n      img-width=\"\"\n    />\n    <pc-feature-card\n      data-translate\n      title=\"\"\n      description=\"It’s all in the wrist. Pair your sensors to your compatible Garmin golf watch.\"\n      image=\"https://res.garmin.com/gdc/Product_Pages/ICONS/pair-1.svg\"\n      img-alt=\"\"\n      img-width=\"\"\n    />\n    <pc-feature-card\n      data-translate\n      title=\"\"\n      description=\"Could you make it on tour? Compare to professional golfers with strokes gained analysis.\"\n      image=\"https://res.garmin.com/gdc/Product_Pages/ICONS/GOLF-LEADERBOARD.svg\"\n      img-alt=\"\"\n      img-width=\"\"\n    />\n    <pc-feature-card\n      data-translate\n      title=\"\"\n      description=\"Hit us with your best shot. View stats for accuracy before every swing.\"\n      image=\"https://res.garmin.com/gdc/Product_Pages/ICONS/GOLF-STATS.svg\"\n      img-alt=\"\"\n      img-width=\"\"\n    />\n    <pc-feature-card\n      data-translate\n      title=\"\"\n      description=\"As your game improves, so will your data. The sensors automatically keep up with you.\"\n      image=\"https://res.garmin.com/gdc/Product_Pages/ICONS/Measure_Data.svg\"\n      img-alt=\"\"\n      img-width=\"\"\n    />\n  </pc-overview-intro>\n  <!-- end overview intro -->\n</div><div>  <!-- start life style -->\n  <pc-life-style\n    data-translate\n    text=\"The more you know, the better you play.\"\n    text-size=\"3\"\n    text-align=\"right\"\n    mobile-text-align=\"right\"\n    mobile-text-position=\"top\"\n    mobile-text-size=\"3\"\n    button-text=\"\"\n    link=\"\"\n    bg-image=\"https://res.garmin.com/en/products/010-01994-00/g/37954-D-1-b8567abe-6e2e-4bb8-80cb-ad061cc2c445.jpg\"\n    mobile-bg-image=\"https://res.garmin.com/en/products/010-01994-00/g/37954-M-1-c8be16d4-b9f6-4e18-80e2-831a69fdaa50.jpg\"\n    img-alt=\"\"\n    img-width=\"\"\n    theme=\"dark\"\n  />\n  <!-- end life style -->\n  <!-- start product feature section -->\n  <pc-layout-container>    <pc-column-layout>\n      <pc-feature-card\n        data-translate\n        title=\"CLUB RECOMMENDATIONS\"\n        description=\"The starter pack is great for tracking chip shots next to the green and putts. Attach an Approach CT10 sensor to your wedges and putter. To keep up with additional clubs, use your watch's (sold separately) AutoShot feature.\"\n        \n        img-alt=\"\"\n        img-width=\"225px\"\n      />\n\n      <pc-feature-card\n        data-translate\n        title=\"Easy to Install\"\n        description=\"The lightweight and secure sensors easily attach to the end of a club’s grip. You won’t even know they’re there while you play.\"\n        \n        img-alt=\"\"\n        img-width=\"225px\"\n      />\n\n      <pc-feature-card\n        data-translate\n        title=\"PAIR TO WATCH\"\n        description=\"Have a compatible Garmin golf watch? Pair it to your sensors, and every time you reach for a club with an Approach CT10, you’ll get the stats on your wrist.\"\n        \n        img-alt=\"\"\n        img-width=\"225px\"\n      />\n\n    </pc-column-layout>    <pc-column-layout>\n      <pc-feature-card\n        data-translate\n        title=\"MORE STATS\"\n        description=\"Pair with the <a href='https://buy.garmin.com/en-AU/AU/p/590068'>Garmin Golf™ app</a><sup>1</sup> for strokes gained analysis, to participate in leaderboards and tournaments, and to gain more data about your game.\"\n        \n        img-alt=\"\"\n        img-width=\"225px\"\n      />\n\n      <pc-feature-card\n        data-translate\n        title=\"PLAY ROUND AFTER ROUND\"\n        description=\"An ingenious automatic on/off feature helps provide battery life of up to 4 years. When it’s time to replace batteries, you can easily do it yourself.\"\n        \n        img-alt=\"\"\n        img-width=\"225px\"\n      />\n\n      <pc-feature-card\n        data-translate\n        title=\"\"\n        description=\"\"\n        \n        img-alt=\"\"\n        img-width=\"225px\"\n      />\n\n    </pc-column-layout>  </pc-layout-container>\n  <!-- end product feature section -->  <!-- start life style -->\n  <pc-life-style\n    data-translate\n    text=\"A better round of golf tees off here.\"\n    text-size=\"3\"\n    text-align=\"right\"\n    mobile-text-align=\"right\"\n    mobile-text-position=\"bottom\"\n    mobile-text-size=\"3\"\n    button-text=\"Learn more\"\n    link=\"https://www.garmin.com/en-AU/garmin-technology/golf-science/\"\n    bg-image=\"https://res.garmin.com/gdc/Product_Pages/LEARN-MORE-CARDS/Golf_Science-LONG-D.jpg\"\n    mobile-bg-image=\"https://res.garmin.com/gdc/Product_Pages/LEARN-MORE-CARDS/Golf_Science-LONG-M.png\"\n    img-alt=\"\"\n    img-width=\"\"\n    theme=\"dark\"\n  />\n  <!-- end life style -->\n  <!-- start learn more section -->\n  <pc-layout-container\n    size=\"\"\n    theme=\"neutral\"\n  >\n    <pc-disclaimer-block>\n      <pc-disclaimer-item\n        data-translate\n        counter=\"1\"\n        content=\"Requires app on your compatible smartphone paired to your Garmin smartwatch.\"\n      />\n    </pc-disclaimer-block>\n  </pc-layout-container>\n  <!-- end learn more section -->\n</div>",
          "title": "Overview"
        },
