      {"locale":"en-AU","storeCode":"AU","devicesTabTitle":"Compatible Devices","productId":"550463","sku":"010-01672-35","salesChat":{"salesChatQueue":"AU General Sales Chat","salesChatLaunchIdentifier":"AUGeneralSalesChat"},"skus":{"010-01672-35":{"productId":"550463","productName":"Oregon® 750t","globalProductName":"Oregon® 750t","productVariation":"With TOPO Australia and New Zealand Maps","pvName":"Oregon® 750t, With TOPO Australia and New Zealand","partNumber":"010-01672-35","showPartNumber":true,"showProductVariationName":false,"subscription":false,"url":"/en-AU/p/550463/pn/010-01672-35","productAvailability":null,"shippingDisclaimer":"Estimated availability is 5 &ndash; 8 weeks.","isForceStockEnabled":false,"isSellable":false,"showAddToCart":false,"productLegalText":"<a href='https://www.garmin.com/en-AU/legal/consumer-warranty-repairs/'>Warranty / Availability of Repair Facilities & Parts</a>","productMarketingSummary":null,"expressWarning":null,"seo":{"title":"Garmin Oregon® 750t | Hiking GPS with Camera ","ogImage":"https://res.garmin.com/en/products/010-01672-30/g/cf-lg.jpg","ogDescription":"Oregon® 750t is a hiking handheld w/GPS & GLONASS reception with built-in Wi-Fi®, 8-megapixel camera & TOPO U.S. 100K maps. "},"currencyCode":null,"images":{"defaultImage":{"altTag":"Oregon® 750t","image":"https://res.garmin.com/en/products/010-01672-30/g/cf-lg.jpg","caption":null},"gallery":[{"altTag":"Oregon® 750t 1","image":"https://res.garmin.com/en/products/010-01672-30/g/pd-01-lg.jpg","caption":null},{"altTag":"Oregon® 750t 2","image":"https://res.garmin.com/en/products/010-01672-30/g/pd-02-lg.jpg","caption":null},{"altTag":"Oregon® 750t 3","image":"https://res.garmin.com/en/products/010-01672-30/g/pd-03-lg.jpg","caption":null},{"altTag":"Oregon® 750t 4","image":"https://res.garmin.com/en/products/010-01672-30/g/pd-04-lg.jpg","caption":null},{"altTag":"Oregon® 750t 5","image":"https://res.garmin.com/en/products/010-01672-30/g/pd-05-lg.jpg","caption":null},{"altTag":"Oregon® 750t 6","image":"https://res.garmin.com/en/products/010-01672-30/g/sc-01-lg.jpg","caption":null},{"altTag":"Oregon® 750t 7","image":"https://res.garmin.com/en/products/010-01672-30/g/sc-02-lg.jpg","caption":null},{"altTag":"Oregon® 750t 8","image":"https://res.garmin.com/en/products/010-01672-30/g/sc-03-lg.jpg","caption":null}],"mediaGallery":[{"publicId":"Product_Images/en/products/010-01672-30/g/cf-lg","mediaType":"image","format":"jpg","position":"0"},{"publicId":"Product_Images/en/products/010-01672-30/g/pd-01-lg","mediaType":"image","format":"jpg","position":"3"},{"publicId":"Product_Images/en/products/010-01672-30/g/pd-02-lg","mediaType":"image","format":"jpg","position":"4"},{"publicId":"Product_Images/en/products/010-01672-30/g/pd-03-lg","mediaType":"image","format":"jpg","position":"5"},{"publicId":"Product_Images/en/products/010-01672-30/g/pd-04-lg","mediaType":"image","format":"jpg","position":"6"},{"publicId":"Product_Images/en/products/010-01672-30/g/pd-05-lg","mediaType":"image","format":"jpg","position":"7"},{"publicId":"Product_Images/en/products/010-01672-30/g/sc-01-lg","mediaType":"image","format":"jpg","position":"13"},{"publicId":"Product_Images/en/products/010-01672-30/g/sc-02-lg","mediaType":"image","format":"jpg","position":"14"},{"publicId":"Product_Images/en/products/010-01672-30/g/sc-03-lg","mediaType":"image","format":"jpg","position":"15"}],"valid":true},"options":{"selected":{"version":["With TOPO Australia and New Zealand Maps"]},"enabled":null,"disabled":null},"actions":{},"productHeaderDetails":"<p>Free one-year Birdseye subscription is no longer available</p>","sidebar":{"quickLinks":{"display":true,"showSoftware":true,"showFAQ":true,"softwareLink":"https://support.garmin.com/?tab=software&partNumber=010-01672-35","content":null},"additionalContainer":null},"tabs":{"overviewTab":{"title":"Overview","content":"<div><p><!-- start key features  --></p>\r\n\r\n<div class=\"top\"><span class=\"features-label\" style=\"padding: 3px 10px; background: #eee; color: #999; font-size: 16px; margin-bottom: 20px; font-weight: 700; text-align: left; float: left; letter-spacing: .5px; text-transform: uppercase;\">Key Features</span>\r\n\r\n<div class=\"row top bottom\">\r\n<div align=\"center\" class=\"span3\"><img height=\"50\" src=\"https://static.garmin.com/en/products/010-01672-30/g/touch.png\" width=\"50\" />\r\n<div class=\"feature\" style=\"font-size: 9.5px; margin-top: 10px; letter-spacing: .5px; font-weight: 400; color: #999999; text-transform: uppercase; line-height: 1.4;\">3\" Touch Screen</div>\r\n</div>\r\n\r\n<div align=\"center\" class=\"span3\"><img height=\"50\" src=\"https://static.garmin.com/en/products/010-01672-30/g/track.png\" width=\"50\" />\r\n<div class=\"feature\" style=\"font-size: 9.5px; margin-top: 10px; letter-spacing: .5px; font-weight: 400; color: #999999; text-transform: uppercase; line-height: 1.4;\">Livetracking</div>\r\n</div>\r\n\r\n<div align=\"center\" class=\"span3\"><img height=\"50\" src=\"https://static.garmin.com/en/products/010-01672-30/g/geo.png\" width=\"50\" />\r\n<div class=\"feature\" style=\"font-size: 9.5px; margin-top: 10px; letter-spacing: .5px; font-weight: 400; color: #999999; text-transform: uppercase; line-height: 1.4;\">Live Geocaching</div>\r\n</div>\r\n\r\n<div align=\"center\" class=\"span3\"><img height=\"50\" src=\"https://static.garmin.com/en/products/010-01672-30/g/camera.png\" width=\"50\" />\r\n<div class=\"feature\" style=\"font-size: 9.5px; margin-top: 10px; letter-spacing: .5px; font-weight: 400; color: #999999; text-transform: uppercase; line-height: 1.4;\">8MP Digital<br />\r\nCamera</div>\r\n</div>\r\n\r\n<div align=\"center\" class=\"span3\"><img height=\"50\" src=\"https://static.garmin.com/en/products/010-01672-30/g/topo.png\" width=\"50\" />\r\n<div class=\"feature\" style=\"font-size: 9.5px; margin-top: 10px; letter-spacing: .5px; font-weight: 400; color: #999999; text-transform: uppercase; line-height: 1.4;\">TOPO MAPPING</div>\r\n</div>\r\n</div>\r\n</div>\r\n\r\n<p><!-- end key features --></p>\r\n\r\n<h3>Rugged GPS/GLONASS Handheld with Built-in Wi-Fi®, Camera and TOPO Lite AU/NZ</h3>\r\n\r\n<ul>\r\n\t<li>Redesigned antenna enables better GPS and GLONASS reception and performance; 3-axis compass with accelerometer and barometric altimeter sensors</li>\r\n\t<li>3-inch sunlight-readable touchscreen with dual orientation (landscape or portrait view)</li>\r\n\t<li>Expanded wireless connectivity (Wi-Fi, Bluetooth®, ANT+® capable); includes smart notifications¹ and automatic uploads to Garmin Connect™ online community and supports Active Weather and Geocaching Live</li>\r\n\t<li>8 megapixel autofocus camera; automatically geotags photos</li>\r\n\t<li>Preloaded TOPO Lite AU/NZ maps</li>\r\n</ul>\r\n\r\n<p><!-- start full-width image --><img alt=\"At a Glance\" border=\"0\" class=\"bottom-s\" height=\"\" src=\"https://static.garmin.com/en/products/010-01672-30/g/at-a-glance.jpg\" width=\"700\" /></p>\r\n\r\n<p><!-- end full-width image --></p>\r\n</div><div><p><!-- start copy block + large image right --></p>\r\n\r\n<div class=\"row top-m bottom-m\">\r\n<div class=\"span8\"><img alt=\"\" border=\"0\" height=\"\" src=\"https://static.garmin.com/en/products/010-01672-30/g/750t-intro.jpg\" width=\"350\" /></div>\r\n\r\n<div class=\"span10\">\r\n<p>Thanks to a redesigned antenna for better satellite reception, the Oregon 750t GPS and GLONASS handheld puts even more of the great outdoors at your fingertips. Whether you’re hiking, hunting, climbing, kayaking, trail riding or whatever, this rugged navigator is ready for anything. It features multilink wireless connectivity (Wi-Fi, Bluetooth and ANT+), active weather support with animated radar overlays and a built-in 3-axis compass with barometric altimeter.</p>\r\n</div>\r\n</div>\r\n\r\n<p><!-- end copy block + large image right --><!-- start copy block + large image right --></p>\r\n\r\n<div class=\"row top-m bottom-m\">\r\n<div class=\"span9\">\r\n<h3>Find Fun with Geocaching.com</h3>\r\n\r\n<p>The wireless Wi-Fi and Bluetooth links on your Oregon 750t makes it easier than ever to enjoy a superior geocaching experience. You can set your device to automatically stay up to date with all the latest caches from <a href=\"http://Geocaching.com\">Geocaching.com</a>. No more manual entry of coordinates or printing out cache details on paper.</p>\r\n</div>\r\n\r\n<div class=\"span9\"><img alt=\"\" border=\"0\" height=\"\" src=\"https://static.garmin.com/en/products/010-01672-30/g/geo.jpg\" width=\"350\" /></div>\r\n</div>\r\n\r\n<p><!-- end copy block + large image right --></p>\r\n\r\n<p><!-- start copy block + large image right --></p>\r\n\r\n<div class=\"row top-m bottom-m\">\r\n<div class=\"span9\">\r\n<h3>More Connectivity, Less Complexity</h3>\r\n\r\n<p>The expanded wireless capabilities of Oregon 750t make it easier than ever to download software, receive smart notifications¹ and upload adventures to <a href=\"https://connect.garmin.com/en-US/\">Garmin Connect</a>. You can even share LiveTrack location data or geocache discoveries, unit-to-unit, with other compatible Garmin handheld devices. Smart notifications via Bluetooth connectivity with a compatible smartphone let you receive emails, texts and alerts right on your device. So you can see what’s going on while you’re getting away from it all. Comes standard with a dual battery system that lets you use traditional AA batteries or a rechargeable NiMH power pack.</p>\r\n</div>\r\n\r\n<div class=\"span9\"><img alt=\"\" border=\"0\" height=\"\" src=\"https://static.garmin.com/en/products/010-01672-30/g/hike.jpg\" width=\"350\" /></div>\r\n</div>\r\n\r\n<p><!-- end copy block + large image right --><!-- start copy block + large image right --></p>\r\n\r\n<div class=\"row top-m bottom-m\">\r\n<div class=\"span9\">\r\n<h3>Take Geotagged Pictures</h3>\r\n\r\n<p>Capture locations and memories with the 8 megapixel digital camera. Each photo is automatically geotagged, allowing you to easily navigate back to that exact spot in the future. You can upload and share your favorite photos with friends via <a href=\"http://adventures.garmin.com/en-US/\">Garmin Adventures</a>™, part of the Garmin online community.</p>\r\n</div>\r\n\r\n<div class=\"span9\"><img alt=\"\" border=\"0\" height=\"\" src=\"https://static.garmin.com/en/products/010-01672-30/g/geotag.jpg\" width=\"350\" /></div>\r\n</div>\r\n\r\n<p><!-- end copy block + large image right --></p>\r\n\r\n<p><!-- start copy block + large image right --></p>\r\n\r\n<div class=\"row top-m bottom-m\">\r\n<div class=\"span9\">\r\n<h3>Navigate Your Way</h3>\r\n\r\n<p>The Oregon 750t handheld provides comprehensive navigation and tracking to guide you on and off the beaten path, using GPS and GLONASS plus ABC (altimeter, barometer and compass) sensor capabilities. The built-in altimeter provides elevation data to accurately monitor ascent and descent, while the barometer can be used to predict weather changes by showing short-term trends in air pressure. The 3-axis electronic compass keeps your bearing, whether you’re moving or not. There’s also a TracBack<sup>®</sup> feature on Oregon 750t to help you navigate back to your starting point the same way you came. You can also mark locations, such as your campsite, vehicle or other points of interest, to which you&#39;d like return navigation.</p>\r\n</div>\r\n\r\n<div class=\"span9\"><img alt=\"\" border=\"0\" height=\"\" src=\"https://static.garmin.com/en/products/010-01672-30/g/nav.jpg\" width=\"350\" /></div>\r\n</div>\r\n\r\n<p><!-- end copy block + large image right --></p>\r\n\r\n<div class=\"row top-m bottom-m\">\r\n<div class=\"span9\">\r\n<h3>Enjoy the View on a Vivid Display</h3>\r\n\r\n<p>The Oregon 750t handheld features a bright, sunlight-readable 3-inch color touchscreen display with dual orientation for landscape or portrait views. It easily supports multiple mapping options such as <a href=\"https://buy.garmin.com/en-US/US/prod6452.html\">BlueChart® g2 HD</a> for water navigation or <a href=\"https://buy.garmin.com/en-US/US/c452-c454-c496-p1.html\">City Navigator<sup>®</sup> NT </a>for turn-by-turn driving directions. Durable and <a href=\"http://www.garmin.com/en-US/legal/waterrating\">water-rated to IPX7</a>, the Oregon series is built to keep delivering vital mapping and tracking data, no matter what the elements throw at you. Plus, you can even customize your Oregon’s display with free downloads of apps, widgets and data fields from the <a href=\"https://apps.garmin.com/en-US/\">Garmin Connect IQ</a>™ online store².</p>\r\n</div>\r\n\r\n<div class=\"span9\"><img alt=\"\" border=\"0\" height=\"\" src=\"https://static.garmin.com/en/products/010-01672-30/g/side.jpg\" width=\"280\" /></div>\r\n</div>\r\n\r\n<p><!-- end copy block + large image right --></p>\r\n\r\n<div style=\"padding-bottom: 20px;\"><iframe allowfullscreen=\"\" frameborder=\"0\" height=\"370\" src=\"https://www.youtube.com/embed/OyF84cAbjQM?rel=0&modestbranding=1&theme=light&wmode=opaque\" width=\"670\"></iframe></div>\r\n\r\n<p class=\"disclaimer-block\">¹See <a href=\"http://support.garmin.com/support/searchSupport/case.faces?caseId=%7bbbfc9660-8f77-11e3-d5f4-000000000000%7d\">Garmin.com/BLE</a> for compatibility<br />\r\n²Apps, widgets and data fields will be introduced when they become available; not all products may be available all the time. Please check our website for current inventory of Connect IQ products</p>\r\n\r\n<p class=\"disclaimer-block\">The Bluetooth® word mark and logos are registered trademarks owned by Bluetooth SIG, Inc. Wi-Fi<sup>®</sup> is a registered trademark of the Wi-Fi Alliance.</p>\r\n</div>"},"mapsTab":{"title":"Maps","content":"<h2>Maps for New Regions</h2><p>Need maps for regions other than the ones that came on your device? See all maps that are compatible with your device.</p><form action=\"\" class=\"app__layout__flex-container\" id=\"form-compatible-maps\" name=\"form-compatible-maps\"><div class=\"app__layout__half\"><label for=\"js__regionSelect\">Where are you going?</label><select id=\"js__regionSelect\" class=\"app__select-box\" autocomplete=\"off\"><option value=\"\" selected=\"selected\">Select Continent</option><option value=\"Africa\">Africa</option><option value=\"Asia\">Asia</option><option value=\"Australia\">Australia Oceania</option><option value=\"Europe\">Europe</option><option value=\"Middle East\">Middle East</option><option value=\"North America\">North America</option><option value=\"South America\">South America</option><option value=\"all\">All</option></select></div><div class=\"app__layout__half\"><label for=\"js__activitySelect\">What type of maps do you need?</label><select id=\"js__activitySelect\" class=\"app__select-box\" autocomplete=\"off\"><option value=\"\" selected=\"selected\">Select Activity</option><option value=\"road\">On the Road</option><option value=\"marine\">On the Water</option><option value=\"topo\">On the Trail</option><option value=\"golf\">On the Green</option><option value=\"all\">All</option></select></div></form><div id=\"compatibleMaps\"></div><hr /><h2>Need a map update?</h2><p>Update the maps that came with your device for the best navigation experience.</p><p><a href=\"http://www.garmin.com/en-AU/maps/updates/\">Check for Updates</a></p>"},"inTheBoxTab":{"title":"In the Box","content":"<ul>\r\n\t<li>Oregon 750t</li>\r\n\t<li>Preloaded with TOPO Lite AU/NZ Maps</li>\r\n\t<li>Rechargable battery pack</li>\r\n\t<li>USB cable</li>\r\n\t<li>AC adapter</li>\r\n\t<li>Carabiner clip</li>\r\n\t<li>Documentation</li>\r\n</ul>\r\n"},"accessoriesTab":{"title":"Accessories","content":null},"specsTab":{"title":"Specs","content":"<div><table>\n        <tbody>\n        <tr class=\"title\">\n            <td colspan=\"2\" style=\"background: #fff; color: #000; border: 1px solid #ddd; border-width: 0px 0px 1px 0px; padding: 0px;\">\n                <h3>General</h3>\n            </td>\n        </tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Dimension</th><td>2.4\" x 4.5\" x 1.3\" (6.1 x 11.4 x 3.3 cm)</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Weight</th><td>7.4 oz (209.8 g) with batteries </td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\"> <a href=\"https://www.garmin.com/en-AU/legal/waterrating-definitions/\">Water rating</a> </th><td>IPX7</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Touchscreen</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Display Type</th><td>transflective color TFT touchscreen</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Display Size</th><td>1.5\"W x 2.5\"H (3.8 x 6.3 cm); 3\" diag (7.6 cm)</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Display Resolution</th><td>241 x 400 pixels</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Battery type</th><td>2 AA batteries (not included); NiMH or Lithium recommended; rechargeable NiMH pack (optional)</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Battery life</th><td>up to 16 hours</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Memory/History</th><td>4 GB (user space varies based on included mapping)</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Interface</th><td>high speed mini USB and NMEA 0183 compatible</td></tr></tbody></table><table>\n        <tbody>\n        <tr class=\"title\">\n            <td colspan=\"2\" style=\"background: #fff; color: #000; border: 1px solid #ddd; border-width: 0px 0px 1px 0px; padding: 0px;\">\n                <h3>Maps & Memory</h3>\n            </td>\n        </tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Preloaded maps</th><td><p>yes (TOPO U.S. 100K; not routable)</p>\r\n</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Ability to add maps</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Basemap</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Automatic routing (turn by turn routing on roads) for outdoor activities</th><td>Yes (with optional mapping for detailed roads)</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Map segments</th><td>15000</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Includes detailed hydrographic features (coastlines, lake/river shorelines, wetlands and perennial and seasonal streams)</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Includes searchable points of interests (parks, campgrounds, scenic lookouts and picnic sites)</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Displays national, state and local parks, forests, and wilderness areas</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">External memory storage</th><td>yes (32 GB max microSD™ card)</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Waypoints/favorites/locations</th><td>10000</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Tracks</th><td>250</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 3