      "showProductVariationName": false,
      "sidebar": {
        "additionalContainer": {
          "content": "<p><!-- begin side content box --></p>\r\n\r\n<div class=\"cssbox\"><a href=\"http://sites.garmin.com/clearvu-scanning/\"><img src=\"//static.garmincdn.com/en/products/010-01253-00/g/21667-Sonar-Landing-Page-Thumbnail.jpg\" /> </a></div>\r\n<!-- end side content box -->\r\n\r\n<div class=\"cssbox\"></div>\r\n\r\n<div class=\"cssbox\"></div>\r\n<!-- begin side content box -->\r\n\r\n<div class=\"cssbox\">\r\n<div class=\"cssbox_head\">\r\n<p>Screen Gallery</p>\r\n</div>\r\n\r\n<div class=\"cssbox_body\"><a href=\"http://www.flickr.com/photos/garmin/sets/72157628438217781/\" target=\"_blank\"><img align=\"left\" alt=\"Sonar Image Gallery\" src=\"https://static.garmincdn.com/en/products/010-00958-00/g/gsd-26-thumbnail.jpg\" style=\"margin-bottom: 20px; float: left; margin-top: 10px; margin-right: 10px\" /></a>\r\n\r\n<p style=\"padding-top: 15px\"><a href=\"http://www.flickr.com/photos/garmin/sets/72157628438217781/\" target=\"_blank\">View</a> more images at flickr<sup>®</sup>.</p>\r\n\r\n<div style=\"clear: both\"></div>\r\n</div>\r\n</div>\r\n<!-- end side content box -->"
        },
        "quickLinks": {
          "content": "<a href=\"http://www8.garmin.com/transducers/\" target=\"_blank\">Transducer Selector</a>\r\n",
          "display": true,
