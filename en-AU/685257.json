:"7"},{"publicId":"Product_Images/en/products/010-02195-00/g/sc-01-lg-0140bba0-215b-4666-b699-f7c5dc5555c5","mediaType":"image","format":"jpg","position":"13"},{"publicId":"Product_Images/en/products/010-02195-00/g/sc-02-lg-ada1f372-d389-4e44-9897-11f7b08d7970","mediaType":"image","format":"jpg","position":"14"},{"publicId":"Product_Images/en/products/010-02195-00/g/sc-03-lg-a58173f9-0787-40ef-be1c-a9ca981d7ae1","mediaType":"image","format":"jpg","position":"15"},{"publicId":"Product_Images/en/products/010-02195-00/g/sc-04-lg-7c02fa54-eb9c-4d3e-9a7e-c409fdfa3283","mediaType":"image","format":"jpg","position":"16"},{"publicId":"Product_Images/en/products/010-02195-00/g/sc-05-lg-d8474067-8bc0-4872-a906-7e0f81dec97c","mediaType":"image","format":"jpg","position":"17"}],"valid":true},"options":{"selected":{"pvemsTreadPowerswitch":["No"]},"enabled":{},"disabled":{}},"actions":{"buy":{"text":"Add To Cart","href":"https://buy.garmin.com/en-AU/cart/addToCart?partNumQty=010-02195-20:1"}},"productHeaderDetails":null,"sidebar":{"quickLinks":{"display":true,"showSoftware":true,"showFAQ":true,"softwareLink":"https://support.garmin.com/?tab=software&partNumber=010-02195-20","content":null},"additionalContainer":null},"tabs":{"overviewTab":{"title":"Overview","content":"<div><overview-container/>\r\n  <!-- start video banner -->\r\n  <pc-video-banner\r\n    data-translate\r\n    video-title=\"Youtube Video\"\r\n    video-link=\"https://www.youtube-nocookie.com/embed/TT0CApWBMAw\"\r\n    bg-image=\"https://res.garmin.com/en/products/010-02195-00/g/_VIDEO-D-d7176b8d-ff9e-4940-9eae-954775c98893.jpg\"\r\n    mobile-bg-image=\"https://res.garmin.com/en/products/010-02195-00/g/_VIDEO-M-571e904b-cae3-4b53-a04b-7ab7f118cad4.jpg\"\r\n    img-alt=\"video\"\r\n    not-youtube=\"false\"\r\n    theme=\"light\"\r\n  />\r\n  <!-- end video banner -->  <!-- start overview intro -->\r\n  <pc-overview-intro\r\n    data-translate\r\n    title=\"ROAM THE UNKNOWN\"\r\n    title-heading=\"h1\"\r\n    title-align=\"center\"\r\n    description=\"Built for every part of the journey, Overlander is the rugged, all-terrain navigator with on- and off-road navigation.\"\r\n    description-size=\"large\"\r\n    description-align=\"center\"\r\n    theme=\"light\"\r\n  >\r\n    <pc-feature-card\r\n      data-translate\r\n      title=\"\"\r\n      description=\"Get the lay of the land with topographic maps for Australia and New Zealand.\"\r\n      image=\"https://res.garmin.com/gdc/Product_Pages/ICONS/TOPO.svg\"\r\n      img-alt=\"\"\r\n      img-width=\"\"\r\n    />\r\n    <pc-feature-card\r\n      data-translate\r\n      title=\"\"\r\n      description=\"Or get spoken directions to the nearest coffee shop with street maps.\"\r\n      image=\"https://res.garmin.com/gdc/Product_Pages/ICONS/COMPASS.svg\"\r\n      img-alt=\"\"\r\n      img-width=\"\"\r\n    />\r\n    <pc-feature-card\r\n      data-translate\r\n      title=\"\"\r\n      description=\"Share<sup>1</sup> the plans for adventure through Garmin Explore. From your laptop to your phone to your friends.\"\r\n      image=\"https://res.garmin.com/gdc/Product_Pages/ICONS/SYNC.svg\"\r\n      img-alt=\"\"\r\n      img-width=\"\"\r\n    />\r\n    <pc-feature-card\r\n      data-translate\r\n      title=\"\"\r\n      description=\"Comes with a powered magnetic mount and an adapter with AMPS plate compatible with RAM mounts. It’s not going anywhere.\"\r\n      image=\"https://res.garmin.com/gdc/Product_Pages/ICONS/MAGNET.svg\"\r\n      img-alt=\"\"\r\n      img-width=\"\"\r\n    />\r\n    <pc-feature-card\r\n      data-translate\r\n      title=\"\"\r\n      description=\"Search for the nearest inspiration point while you’re in the middle of nowhere.\"\r\n      image=\"https://res.garmin.com/gdc/Product_Pages/ICONS/CAMP-TENT.svg\"\r\n      img-alt=\"\"\r\n      img-width=\"\"\r\n    />\r\n    <pc-feature-card\r\n      data-translate\r\n      title=\"\"\r\n      description=\"Need to bring your own satellite imagery? How does 32 GB of built-in storage sound?\"\r\n      image=\"https://res.garmin.com/gdc/Product_Pages/ICONS/DIGITAL-STORAGE.svg\"\r\n      img-alt=\"\"\r\n      img-width=\"\"\r\n    />\r\n  </pc-overview-intro>\r\n  <!-- end overview intro -->\r\n</div><div>  <!-- start life style -->\r\n  <pc-life-style\r\n    data-translate\r\n    text=\"Built tough for the journey ahead.\"\r\n    text-size=\"3\"\r\n    text-align=\"left\"\r\n    mobile-text-align=\"left\"\r\n    mobile-text-position=\"top\"\r\n    mobile-text-size=\"3\"\r\n    button-text=\"\"\r\n    link=\"\"\r\n    bg-image=\"https://res.garmin.com/en/products/010-02195-00/g/_1-D-e2dafc46-c34c-4db6-8786-f52cc4453a7e.jpg\"\r\n    mobile-bg-image=\"https://res.garmin.com/en/products/010-02195-00/g/_1-M-aaf1458b-a13c-4e81-8989-fb55a1922a74.jpg\"\r\n    img-alt=\"\"\r\n    img-width=\"\"\r\n    theme=\"dark\"\r\n  />\r\n  <!-- end life style -->\r\n  <!-- start product feature section -->\r\n  <pc-layout-container>    <pc-column-layout>\r\n      <pc-feature-card\r\n        data-translate\r\n        title=\"ON AND OFF ROAD\"\r\n        description=\"7″ colour touchscreen features turn-by-turn directions for on-road and off-road navigation covering Australia and New Zealand. Includes 32 GB internal memory to download additional TOPO maps.\"\r\n        image=\"https://res.garmin.com/en/products/010-02195-00/g/_1-d5fd8be1-43d9-400b-b113-475972a226e0.jpg\"\r\n        img-alt=\"\"\r\n        img-width=\"300px\"\r\n      />\r\n\r\n      <pc-feature-card\r\n        data-translate\r\n        title=\"MILITARY STANDARD 810\"\r\n        description=\"Overlander is built to the U.S. military standard 810 for thermal and shock resistance. It can be mounted anywhere, thanks to the powered magnetic mount that comes with suction cup and 1″ ball adapter with AMPS plate compatible with RAM mounting systems.\"\r\n        image=\"https://res.garmin.com/en/products/010-02195-00/g/_2-4eae414a-41d2-4d03-961a-dadd90627f88.jpg\"\r\n        img-alt=\"\"\r\n        img-width=\"300px\"\r\n      />\r\n\r\n      <pc-feature-card\r\n        data-translate\r\n        title=\"IP5X DUST RATING\"\r\n        description=\"This essential piece of overlanding gear is purpose-built from the circuit board up and is designed to withstand dust and extreme temperatures.\"\r\n        image=\"https://res.garmin.com/en/products/010-02195-00/g/_3-aa26fdb3-18e7-4646-98b0-f70f9ae7d473.jpg\"\r\n        img-alt=\"\"\r\n        img-width=\"300px\"\r\n      />\r\n\r\n    </pc-column-layout>  </pc-layout-container>\r\n  <!-- end product feature section -->  <!-- start life style -->\r\n  <pc-life-style\r\n    data-translate\r\n    text=\"Go beyond the road less travelled.\"\r\n    text-size=\"3\"\r\n    text-align=\"right\"\r\n    mobile-text-align=\"right\"\r\n    mobile-text-position=\"top\"\r\n    mobile-text-size=\"3\"\r\n    button-text=\"\"\r\n    link=\r\n    bg-image=\"https://res.garmin.com/en/products/010-02195-00/g/_2-D-32b90224-4cc0-4a96-8780-439e9a8326d4.jpg\"\r\n    mobile-bg-image=\"https://res.garmin.com/en/products/010-02195-00/g/_2-M-20f28803-3a96-45d9-8042-d311ae1bd89d.jpg\"\r\n    img-alt=\"\"\r\n    img-width=\"\"\r\n    theme=\"dark\"\r\n  />\r\n  <!-- end life style -->\r\n  <!-- start product feature section -->\r\n  <pc-layout-container>    <pc-column-layout>\r\n      <pc-feature-card\r\n        data-translate\r\n        title=\"GARMIN EXPLORE\"\r\n        description=\"Use the <a href='https://buy.garmin.com/en-AU/AU/p/614327'>Garmin Explore</a> app to download additional maps – such as satellite imagery and Ultimate Public Campgrounds on 32 GB of internal storage – and sync all your data between devices. Easily import and export GPX files to Garmin Explore — even if offline.\"\r\n        image=\"https://res.garmin.com/en/products/010-02195-00/g/_4-f8ad1fb2-ff58-4066-8585-9ed2c179c5fb.jpg\"\r\n        img-alt=\"\"\r\n        img-width=\"300px\"\r\n      />\r\n\r\n      <pc-feature-card\r\n        data-translate\r\n        title=\"FIND BOUNDARIES\"\r\n        description=\"Overlander features preloaded topography for Australia and New Zealand. Download additional topographic maps worldwide on the 32 GB of internal storage.\"\r\n        image=\"https://res.garmin.com/en/products/010-02195-00/g/_6-d2e643e4-356e-436a-aa6f-cb64aec522ce.jpg\"\r\n        img-alt=\"\"\r\n        img-width=\"300px\"\r\n      />\r\n\r\n      <pc-feature-card\r\n        data-translate\r\n        title=\"FIND CAMPSITES\"\r\n        description=\"Overlander is preloaded with iOverlander POIs and Ultimate Public Campgrounds, so you don’t need a cell signal to route to the best-established, wild and dispersed campsites.\"\r\n        image=\"https://res.garmin.com/en/products/010-02195-00/g/_5-17420ddc-e1c9-4a47-99f2-8adb9c9b48b6.jpg\"\r\n        img-alt=\"\"\r\n        img-width=\"300px\"\r\n      />\r\n\r\n    </pc-column-layout>    <pc-column-layout>\r\n      <pc-feature-card\r\n        data-translate\r\n        title=\"KNOW YOUR VEHICLE\"\r\n        description=\"Customise street navigation for the size and weight of your overlanding vehicle, plus get alerts for upcoming bridge heights, weight limits, sharp curves, steep grades and more.\"\r\n        image=\"https://res.garmin.com/en/products/010-02195-00/g/_7-bdeff48d-843c-4ba4-ba91-6b9645067456.jpg\"\r\n        img-alt=\"\"\r\n        img-width=\"300px\"\r\n      />\r\n\r\n      <pc-feature-card\r\n        data-translate\r\n        title=\"MULTI-GNSS SUPPORT\"\r\n        description=\"GPS, GLONASS and Galileo support helps track in more challenging environments than GPS alone.\"\r\n        image=\"https://res.garmin.com/en/products/010-02195-00/g/_8-04f517a0-cade-47f7-b09d-67db7aa53d4b.jpg\"\r\n        img-alt=\"\"\r\n        img-width=\"300px\"\r\n      />\r\n\r\n      <pc-feature-card\r\n        data-translate\r\n        title=\"NAVIGATION TOOLS\"\r\n        description=\"All of your overlanding equipment – such as a built-in compass, altimeter and barometer – is now in one device.\"\r\n        image=\"https://res.garmin.com/en/products/010-02195-00/g/Overlander-FPC-NAV-cdffc413-9b70-4180-aa34-e00099e2f074.jpg\"\r\n        img-alt=\"\"\r\n        img-width=\"300px\"\r\n      />\r\n\r\n    </pc-column-layout>  </pc-layout-container>\r\n  <!-- end product feature section -->  <!-- start life style -->\r\n  <pc-life-style\r\n    data-translate\r\n    text=\"The right tools for the roughest roads.\"\r\n    text-size=\"3\"\r\n    text-align=\"left\"\r\n    mobile-text-align=\"left\"\r\n    mobile-text-position=\"top\"\r\n    mobile-text-size=\"3\"\r\n    button-text=\"\"\r\n    link=\"\"\r\n    bg-image=\"https://res.garmin.com/en/products/010-02195-00/g/_3-D-cbdc05e8-a172-4dbf-b0ba-81d92192aafe.jpg\"\r\n    mobile-bg-image=\"https://res.garmin.com/en/products/010-02195-00/g/_3-M-f2dbc9cf-97d2-488d-a8f9-03597adcd81b.jpg\"\r\n    img-alt=\"\"\r\n    img-width=\"\"\r\n    theme=\"dark\"\r\n  />\r\n  <!-- end life style -->\r\n  <!-- start product feature section -->\r\n  <pc-layout-container>    <pc-column-layout>\r\n      <pc-feature-card\r\n        data-translate\r\n        title=\"TRACK RECORDER\"\r\n        description=\"Record your tracks, and save them to your Garmin Explore account so you can review them later on any device or share them with friends.\"\r\n        image=\"https://res.garmin.com/en/products/010-02195-00/g/_10-5cd513a8-dd75-47d5-b09b-fb85abef8d5f.jpg\"\r\n        img-alt=\"\"\r\n        img-width=\"300px\"\r\n      />\r\n\r\n      <pc-feature-card\r\n        data-translate\r\n        title=\"PITCH AND ROLL\"\r\n        description=\"Overlander comes with integrated pitch and roll angles to help you and your vehicle navigate difficult terrain.\"\r\n        image=\"https://res.garmin.com/en/products/010-02195-00/g/_13-9193c546-7937-4b0b-8401-f22e7109d7b3.jpg\"\r\n        img-alt=\"\"\r\n        img-width=\"300px\"\r\n      />\r\n\r\n      <pc-feature-card\r\n        data-translate\r\n        title=\"INREACH® COMPATIBLE\"\r\n        description=\"Pair your Overlander navigator with the <a href='https://buy.garmin.com/en-AU/AU/p/561269/'>inReach Explorer®+,</a>  <a href='https://buy.garmin.com/en-AU/AU/p/561286/'>inReach SE®+</a>  and <a href='https://buy.garmin.com/en-AU/AU/p/592606/'>inReach® Mini</a> satellite communicators<sup>2</sup> (sold separately) for two-way text messaging, interactive SOS and weather forecasts.\"\r\n        image=\"https://res.garmin.com/en/products/010-02195-00/g/_11-49db5cf5-596d-4662-a12b-1f13c209b7ba.jpg\"\r\n        img-alt=\"\"\r\n        img-width=\"300px\"\r\n      />\r\n\r\n    </pc-column-layout>      </pc-layout-container>\r\n  <!-- end product feature section -->  <!-- start life style -->\r\n  <pc-life-style\r\n    data-translate\r\n    text=\"Garmin proudly supports Tread Lightly! and responsible outdoor recreation\"\r\n    text-size=\"2\"\r\n    text-align=\"left\"\r\n    mobile-text-align=\"left\"\r\n    mobile-text-position=\"top\"\r\n    mobile-text-size=\"2\"\r\n    button-text=\"\"\r\n    link=\"\"\r\n    bg-image=\"https://res.garmin.com/gdc/Product_Pages/LEARN-MORE-CARDS/TREAD-LIGHTLY-L-D.jpg\"\r\n    mobile-bg-image=\"https://res.garmin.com/gdc/Product_Pages/LEARN-MORE-CARDS/TREAD-LIGHTLY-L-M.jpg\"\r\n    img-alt=\"\"\r\n    img-width=\"\"\r\n    theme=\"dark\"\r\n  />\r\n  <!-- end life style -->\r\n  <!-- start learn more section -->\r\n  <pc-layout-container\r\n    size=\"\"\r\n    theme=\"neutral\"\r\n  >\r\n    <pc-disclaimer-block>\r\n      <pc-disclaimer-item\r\n        data-translate\r\n        counter=\"1\"\r\n        content=\"Requires an active connection enabled with Wi-Fi® wireless technology.\"\r\n      />\r\n      <pc-disclaimer-item\r\n        data-translate\r\n        counter=\"2\"\r\n        content=\"Active satellite subscription required for inReach device.\"\r\n      />\r\n      <pc-disclaimer-item\r\n        data-translate\r\n        counter=\"\"\r\n        content=\"Wi-Fi® is a registered trademark of the Wi-Fi Alliance.\"\r\n      />\r\n    </pc-disclaimer-block>\r\n  </pc-layout-container>\r\n  <!-- end learn more section -->\r\n</div>"},"mapsTab":{"title":"Maps","content":"<h2>Maps for New Regions</h2><p>Need maps for regions other than the ones that came on your device? See all maps that are compatible with your device.</p><form action=\"\" class=\"app__layout__flex-container\" id=\"form-compatible-maps\" name=\"form-compatible-maps\"><div class=\"app__layout__half\"><label for=\"js__regionSelect\">Where are you going?</label><select id=\"js__regionSelect\" class=\"app__select-box\" autocomplete=\"off\"><option value=\"\" selected=\"selected\">Select Continent</option><option value=\"Africa\">Africa</option><option value=\"Asia\">Asia</option><option value=\"Australia\">Australia Oceania</option><option value=\"Europe\">Europe</option><option value=\"Middle East\">Middle East</option><option value=\"North America\">North America</option><option value=\"South America\">South America</option><option value=\"all\">All</option></select></div><div class=\"app__layout__half\"><label for=\"js__activitySelect\">What type of maps do you need?</label><select id=\"js__activitySelect\" class=\"app__select-box\" autocomplete=\"off\"><option value=\"\" selected=\"selected\">Select Activity</option><option value=\"road\">On the Road</option><option value=\"marine\">On the Water</option><option value=\"topo\">On the Trail</option><option value=\"golf\">On the Green</option><option value=\"all\">All</option></select></div></form><div id=\"compatibleMaps\"></div><hr /><h2>Need a map update?</h2><p>Update the maps that came with your device for the best navigation experience.</p><p><a href=\"http://www.garmin.com/en-AU/maps/updates/\">Check for Updates</a></p>"},"inTheBoxTab":{"title":"In the Box","content":"<ul>\r\n\t<li>Overlander</li>\r\n\t<li>Vehicle suction cup mount</li>\r\n\t<li>Traffic receiver/Vehicle power cable</li>\r\n\t<li>USB cable</li>\r\n\t<li>Documentation</li>\r\n</ul>\r\n"},"accessoriesTab":{"title":"Accessories","content":null},"specsTab":{"title":"Specs","content":"<div><table>\n        <tbody>\n        <tr class=\"title\">\n            <td colspan=\"2\" style=\"background: #fff; color: #000; border: 1px solid #ddd; border-width: 0px 0px 1px 0px; padding: 0px;\">\n                <h3>General</h3>\n            </td>\n        </tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Dimension</th><td>W x H x D 19.9 x 12.2 x 2.4 cm</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Weight</th><td>437 g</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Display Type</th><td>Multi-touch, glass, WSVGA colour TFT with white backlight</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Display Size</th><td>W x H 15.2 x 8.9 cm; 7\" diagonal</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Display Resolution</th><td>1024 x 600 pixels</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Magnetic Mount</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">1\" ball adapter with AMPS plate</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Drop rating</th><td>MIL-STD-810</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Dust rating</th><td>IP5X</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Battery type</th><td>Rechargeable lithium-ion</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Battery life</th><td>Up to 3 hours</td></tr></tbody></table><table>\n        <tbody>\n        <tr clas