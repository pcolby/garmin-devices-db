ate />\n      </pc-table-row>\n      <pc-table-row>\n          <pc-table-cell text=\"Tacx challenges\" align=\"left\" data-translate />\n          <pc-table-cell text=\"\" align=\"center\" data-translate />\n          <pc-table-cell checkmark=\"true\" data-translate />\n          <pc-table-cell checkmark=\"true\" data-translate />\n      </pc-table-row>\n      <pc-table-row>\n          <pc-table-cell text=\"3D GPS workouts\" align=\"left\" data-translate />\n          <pc-table-cell text=\"\" align=\"center\" data-translate />\n          <pc-table-cell checkmark=\"true\" data-translate />\n          <pc-table-cell checkmark=\"true\" data-translate />\n      </pc-table-row>\n      <pc-table-row>\n          <pc-table-cell text=\"Live opponents\" align=\"left\" data-translate />\n          <pc-table-cell text=\"\" align=\"center\" data-translate />\n          <pc-table-cell checkmark=\"true\" data-translate />\n          <pc-table-cell checkmark=\"true\" data-translate />\n      </pc-table-row>\n    </pc-table>\n  </pc-layout-container>\n  <!-- end table -->\n  <!-- start learn more section -->\n  <pc-layout-container\n    size=\"medium\"\n    theme=\"neutral\"\n  >\n    <pc-column-layout\n      columns=\"2\"\n    >\n      <pc-learn-more\n        data-translate\n        title=\"See your stats, and track your goals in the Garmin Connect™ app.\"\n        description=\"\"\n        button-text=\"Learn more\"\n        link=\"https://www.garmin.com/en-AU/p/125677\"\n        bg-image=\"https://res.garmin.com/gdc/Product_Pages/LEARN-MORE-CARDS/HALF-CONNECT-D.png\"\n        mobile-bg-image=\"https://res.garmin.com/gdc/Product_Pages/LEARN-MORE-CARDS/HALF-CONNECT-M.png\"\n        img-alt=\"\"\n        theme=\"light\"\n      />\n      <pc-learn-more\n        data-translate\n        title=\"Meet the pros who use Garmin to power their passions.\"\n        description=\"\"\n        button-text=\"Learn more\"\n        link=\"https://www.garmin.com/en-AU/pros/\"\n        bg-image=\"htt