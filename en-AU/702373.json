      {"locale":"en-AU","storeCode":"AU","devicesTabTitle":"Compatible Devices","productId":"702373","sku":"010-02406-20","salesChat":{"salesChatQueue":null,"salesChatLaunchIdentifier":null},"skus":{"010-02406-20":{"productId":"702373","productName":"Tread®","globalProductName":"Tread®","productVariation":"Tread™","pvName":"Tread™","partNumber":"010-02406-20","showPartNumber":true,"showProductVariationName":false,"subscription":false,"url":"/en-AU/p/702373/pn/010-02406-20","productAvailability":null,"shippingDisclaimer":"Ready to ship in 1&ndash;3 business days.","isForceStockEnabled":false,"isSellable":true,"showAddToCart":true,"productLegalText":"<a href='https://www.garmin.com/en-AU/legal/consumer-warranty-repairs/'>Warranty / Availability of Repair Facilities & Parts</a>","productMarketingSummary":null,"expressWarning":null,"seo":{"title":"Garmin Tread™ | Powersport GPS with Ride Tracker","ogImage":"https://res.garmin.com/en/products/010-02406-20/v/cf-lg-e23d8ff8-7630-4d15-98be-3f857deda33b.jpg","ogDescription":"Tread is a Powersport GPS that empowers you to roam the unknown. Features include pre-loaded topography mapping and a ride tracker  to stay connected with the group."},"currencyCode":"AUD","images":{"defaultImage":{"altTag":"Tread®","image":"https://res.garmin.com/en/products/010-02406-20/v/cf-lg-e23d8ff8-7630-4d15-98be-3f857deda33b.jpg","caption":null},"gallery":[{"altTag":"Tread® 1","image":"https://res.garmin.com/en/products/010-02406-00/g/cf-lg-162925bb-96c5-4386-9292-a81e444dcc17.jpg","caption":null},{"altTag":"Tread® 2","image":"https://res.garmin.com/en/products/010-02406-00/g/pd-01-lg-9545cd83-9524-4e2d-8f34-0c7e3637703d.jpg","caption":null},{"altTag":"Tread® 3","image":"https://res.garmin.com/en/products/010-02406-00/g/pd-02-lg-e9a070af-e3e8-440f-8f33-7052a2aa70ea.jpg","caption":null},{"altTag":"Tread® 4","image":"https://res.garmin.com/en/products/010-02406-00/g/pd-03-lg-6e072cf5-1121-435a-9d75-d14ff47c1659.jpg","caption":null},{"altTag":"Tread® 5","image":"https://res.garmin.com/en/products/010-02406-00/g/pd-04-lg-40129407-4949-4c1d-b0ee-638d225e9c83.jpg","caption":null},{"altTag":"Tread® 6","image":"https://res.garmin.com/en/products/010-02406-00/g/pd-05-lg-e68e079e-8176-4a4e-ab25-6d108b283c54.jpg","caption":null}],"mediaGallery":[{"publicId":"Product_Images/en/products/010-02406-20/v/cf-lg-e23d8ff8-7630-4d15-98be-3f857deda33b","mediaType":"image","format":"jpg","position":"0"},{"publicId":"Product_Images/en/products/010-02406-20/v/rf-lg-cfd52d2a-a870-4ccf-817f-5b02d6ebca05","mediaType":"image","format":"jpg","position":"1"},{"publicId":"Product_Images/en/products/010-02406-20/v/lf-lg-147f9cb1-9936-479b-a450-4b1f1f461576","mediaType":"image","format":"jpg","position":"2"},{"publicId":"Product_Images/en/products/010-02406-20/v/pd-01-lg-efcd7db8-4e49-4131-b38f-6a5d88d8436e","mediaType":"image","format":"jpg","position":"3"},{"publicId":"Product_Images/en/products/010-02406-20/v/pd-02-lg-6526c2cb-6870-4549-b04b-68968a29fb59","mediaType":"image","format":"jpg","position":"4"},{"publicId":"Product_Images/en/products/010-02406-20/v/pd-03-lg-54bfcca0-4482-423b-ace0-e916b4e86732","mediaType":"image","format":"jpg","position":"5"},{"publicId":"Product_Images/en/products/010-02406-20/v/pd-04-lg-df9bde5d-6f27-4242-9f3f-05c3b204cdb1","mediaType":"image","format":"jpg","position":"6"},{"publicId":"Product_Images/en/products/010-02406-20/v/pd-05-lg-2bcb9422-e5f6-4dd7-ae33-136a16c729a5","mediaType":"image","format":"jpg","position":"7"}],"valid":true},"options":{"selected":{"version":["Tread™"]},"enabled":null,"disabled":null},"actions":{"buy":{"text":"Add To Cart","href":"https://buy.garmin.com/en-AU/cart/addToCart?partNumQty=010-02406-20:1"}},"productHeaderDetails":null,"sidebar":{"quickLinks":{"display":true,"showSoftware":true,"showFAQ":true,"softwareLink":"https://support.garmin.com/?tab=software&partNumber=010-02406-20","content":null},"additionalContainer":null},"tabs":{"overviewTab":{"title":"Overview","content":"<div><overview-container/>\n\n  <!-- start video banner -->\n  <pc-video-banner\n    data-translate\n    video-title=\"Youtube Video\"\n    video-link=\"https://www.youtube-nocookie.com/embed/NiMnhXhDev4\"\n    bg-image=\"https://res.garmin.com/en/products/010-02406-00/g/32245-D-VID-398064a9-f451-4b38-aafd-9578bbbd4ec2.jpg\"\n    mobile-bg-image=\"https://res.garmin.com/en/products/010-02406-00/g/32245-M-3EU-5d08f559-2675-4df5-b8c3-dea8b227c7e1.jpg\"\n    img-alt=\"video\"\n    not-youtube=\"false\"\n    theme=\"dark\"\n  />\n  <!-- end video banner -->\n  <!-- start overview intro -->\n  <pc-overview-intro\n    data-translate\n    title=\"ROAM THE UNKNOWN\"\n    title-heading=\"h1\"\n    title-align=\"center\"\n    description=\"Spend your time ripping through the woods, not waiting on your friends. This rugged powersport GPS sat nav keeps your groups in touch and on track.\"\n    description-size=\"large\"\n    description-align=\"center\"\n    theme=\"light\"\n  >\n    <pc-feature-card\n      data-translate\n      title=\"\"\n      description=\"Keep your crew together with group tracking for up to 20 riders, plus location sharing<sup>1</sup>.\"\n      image=\"https://res.garmin.com/gdc/Product_Pages/ICONS/Grouptracks.svg\"\n      img-alt=\"\"\n      img-width=\"50px\"\n    />\n    <pc-feature-card\n      data-translate\n      title=\"\"\n      description=\"Navigate unpaved roads and trails by using turn-by-turn trail navigation<sup>2</sup> with OpenStreetMap (OSM).\"\n      image=\"https://res.garmin.com/gdc/Product_Pages/ICONS/TOPO.svg\"\n      img-alt=\"\"\n      img-width=\"50px\"\n    />\n    <pc-feature-card\n      data-translate\n      title=\"\"\n      description=\"The rugged Tread off-road GPS is <a href='https://www.garmin.com/en-AU/legal/waterrating-definitions/'>IPX7 weather resistant</a> and features a glove-friendly 5.5&Prime; touchscreen display.\"\n      image=\"https://res.garmin.com/gdc/Product_Pages/ICONS/water-drop-resistant.svg\"\n      img-alt=\"\"\n      img-width=\"50px\"\n    />\n    <pc-feature-card\n      data-translate\n      title=\"\"\n      description=\"Use the <a href='https://www.garmin.com/en-AU/p/773341'>Tread<sup>®</sup> app<sup>3</sup></a> to sync data across your various devices.\"\n      image=\"https://res.garmin.com/gdc/Product_Pages/ICONS/Smartphone.svg\"\n      img-alt=\"\"\n      img-width=\"50px\"\n    />\n    <pc-feature-card\n      data-translate\n      title=\"\"\n      description=\"Direct-to-device BirdsEye Satellite Imagery can be downloaded to Tread without a computer.\"\n      image=\"https://res.garmin.com/gdc/Product_Pages/ICONS/Birdseye.svg\"\n      img-alt=\"\"\n      img-width=\"50px\"\n    />\n    <pc-feature-card\n      data-translate\n      title=\"\"\n      description=\"Easily install on your powersport vehicle with the included powered mount and wiring harness with tube mount.\"\n      image=\"https://res.garmin.com/gdc/Product_Pages/ICONS/Mount.svg\"\n      img-alt=\"\"\n      img-width=\"50px\"\n    />\n  </pc-overview-intro>\n  <!-- end overview intro -->\n</div><div>  <!-- start life style -->\n  <pc-life-style\n    data-translate\n    text=\"Gear up for group rides.\"\n    text-size=\"2.6\"\n    text-align=\"left\"\n    text-position=\"middle\"\n    mobile-text-size=\"2.4\"\n    mobile-text-align=\"center\"\n    mobile-text-position=\"bottom\"\n    button-text=\"\"\n    link=\"\"\n    bg-image=\"https://res.garmin.com/en/products/010-02406-00/g/EMEA_32245-D-1-bbc4f4b9-ce35-4a7a-ad35-a138da835903.jpg\"\n    mobile-bg-image=\"https://res.garmin.com/en/products/010-02406-00/g/EMEA_32245-M-1-ba988750-4102-49c9-a83b-fb45abfd44cf.jpg\"\n    img-alt=\"\"\n    img-width=\"\"\n    theme=\"dark\"\n  />\n  <!-- end life style -->\n  <!-- start product feature section -->\n  <pc-layout-container>\n    <pc-column-layout\n      columns=\"3\"\n    >\n      <pc-feature-card\n        data-translate\n        title=\"RIDING TRACKER FOR GROUP GETAWAYS\"\n        description=\"The included Group Ride Tracker allows group tracking for up to 20 riders with no mobile phone signal coverage needed. Plus, location sharing and pre-defined messaging<sup>1</sup> lets you easily stay connected with your Group Ride friends <a href='https://www.garmin.com/en-AU/p/702373#additional'>(VHF range).</a> \"\n        image=\"https://res.garmin.com/en/products/010-02406-00/g/32245-1-85a49d96-78f9-4119-8d02-1487c2613d70.jpg\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n      <pc-feature-card\n        data-translate\n        title=\"OFF-ROAD RUGGEDNESS\"\n        description=\"Purpose-built for all-terrain use, Tread stands up to anything the elements can throw at it. It’s <a href='https://www.garmin.com/en-AU/legal/waterrating-definitions/'>IPX7-rated for water resistance</a> and built to U.S. military standard 810 for thermal and shock resistance.\"\n        image=\"https://res.garmin.com/en/products/010-02406-00/g/32245-2-4a4430c3-b7a4-4499-8d92-609375b00188.jpg\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n      <pc-feature-card\n        data-translate\n        title=\"HIGH-VISIBILITY DISPLAY\"\n        description=\"With its ultrabright 5.5&Prime; glove-friendly touchscreen, the Tread sat nav is designed for easy viewing in bright sunlight or pitch-dark. It can be mounted in portrait or landscape mode.\"\n        image=\"https://res.garmin.com/en/products/010-02406-00/g/32245-3-1328d9e9-05ad-4296-a5b7-2a1d2c7bcdbe.jpg\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n    </pc-column-layout>\n    <pc-column-layout\n      columns=\"3\"\n    >\n      <pc-feature-card\n        data-translate\n        title=\"TRAIL NAVIGATION\"\n        description=\"Enjoy turn-by-turn trail navigation<sup>2</sup> for traversing unpaved roads, trails and snowmobile tracks using adventure roads and trails map content comprised of OSM maps.\"\n        image=\"https://res.garmin.com/en_US/products/010-02406-00/EMEA_Tread_Trail_Navigation-cba0ff41-429f-4691-868a-85f9a6c6a6bf.png\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n      <pc-feature-card\n        data-translate\n        title=\"TOPOGRAPHY\"\n        description=\" Tread features preloaded topographic and street maps of Europe, Africa including Tracks4Africa, Middle East, North and South America, Australia and New Zealand.\"\n        image=\"https://res.garmin.com/en/products/010-02406-00/g/32245-R-1-TOPO-3ecb8d2b-5d1a-47f6-a3d8-efc22c1d67ab.jpg\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n      <pc-feature-card\n        data-translate\n        title=\"SATELLITE IMAGERY\"\n        description=\"Download high-resolution BirdsEye Satellite Imagery over <span style='white-space: nowrap; font-weight:inherit;'>Wi-Fi</span> directly to this navigation device; no annual subscription required.\"\n        image=\"https://res.garmin.com/en/products/010-02406-00/g/32245-R-2-IMAGERY-49cd1b1f-4a04-44f9-8ddd-34c0fc3614c2.jpg\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n    </pc-column-layout>\n  </pc-layout-container>\n  <!-- end product feature section -->\n  <!-- start life style -->\n  <pc-life-style\n    data-translate\n    text=\"Track through the outback — and stay in touch with your friends.\"\n    text-size=\"2.6\"\n    text-align=\"right\"\n    text-position=\"middle\"\n    mobile-text-size=\"2.4\"\n    mobile-text-align=\"center\"\n    mobile-text-position=\"bottom\"\n    button-text=\"\"\n    link=\"\"\n    bg-image=\"https://res.garmin.com/en/products/010-02406-00/g/32245-D-2-5b2568da-ce77-491d-8d67-1a76fa80f67b.jpg\"\n    mobile-bg-image=\"https://res.garmin.com/en/products/010-02406-00/g/32245-M-2EU-c05cc76b-13de-4796-9a05-541dbf874789.jpg\"\n    img-alt=\"\"\n    img-width=\"\"\n    theme=\"dark\"\n  />\n  <!-- end life style -->\n  <!-- start product feature section -->\n  <pc-layout-container>\n    <pc-column-layout\n      columns=\"3\"\n    >\n      <pc-feature-card\n        data-translate\n        title=\"Group ride mobile\"\n        description=\"Pair your device with the Tread app to track your friends from your phone using the Group Ride Mobile feature.\"\n        image=\"https://res.garmin.com/en_US/products/010-02406-00/EMEA_Tread_Group_Ride_Mobile-cba0ff41-429f-4691-868a-85f9a6c6a6bf.png\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n      <pc-feature-card\n        data-translate\n        title=\"EASY ROUTE PLANNER\"\n        description=\"Use the Tread app to easily plan routes with the same maps, vehicle profiles and route preferences that you have on the Tread device.\"\n        image=\"https://res.garmin.com/en_US/products/010-02406-00/EMEA_Tread_Route_Planner-cba0ff41-429f-4691-868a-85f9a6c6a6bf.png\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n      <pc-feature-card\n        data-translate\n        title=\"EASILY SYNC YOUR DEVICES\"\n        description=\"Use the <a href='https://www.garmin.com/en-AU/p/773341'>Tread app<sup>3</sup></a> to sync waypoints, tracks, routes and collections across your devices, including your mobile device, computer and Tread sat nav. Easily import and share GPX files, too.\"\n        image=\"https://res.garmin.com/en/products/010-02406-00/g/32245-7-5c711ab5-3d89-4281-8b2d-b1fdcbc98872.jpg\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n    </pc-column-layout>\n    <pc-column-layout\n      columns=\"3\"\n    >\n      <pc-feature-card\n        data-translate\n        title=\"MOUNTS WITH MUSCLE\"\n        description=\" Includes a powered mount, wiring harness with tube mount and a handlebar mount to secure your Tread sat nav to the powersport vehicle for riding over rough terrain.\"\n        image=\"https://res.garmin.com/en/products/010-02406-00/g/32245-8-07c8ae03-f4a8-4956-8904-79f34becf3ed.jpg\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n      <pc-feature-card\n        data-translate\n        title=\"ABC SENSORS\"\n        description=\"For tough-terrain guidance, built-in sensors include an altimeter, barometer, compass and pitch/roll gauges.\"\n        image=\"https://res.garmin.com/en/products/010-02406-00/g/32245-10-f2be4b59-3651-4ac4-bcd6-6c362d8cd7ec.jpg\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n      <pc-feature-card\n        data-translate\n        title=\"INREACH® COMPATIBLE\"\n        description=\"Pair your Tread powersport sat nav with one of the compatible <a href='https://discover.garmin.com/en-AU/inreach/personal/#subscriptions'>inReach</a> global satellite communicators<sup>4</sup> (sold separately) for two-way text messaging, interactive SOS and weather.\"\n        image=\"https://res.garmin.com/en/products/010-02406-00/g/32245-11-db6d91da-7b0d-4fa4-b4d9-5ca9658b3e02.jpg\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n    </pc-column-layout>\n    <pc-column-layout\n      columns=\"3\"\n    >\n      <pc-feature-card\n        data-translate\n        title=\"DOG TRACKING\"\n        description=\"Wirelessly pair Tread with select Garmin GPS dog systems (sold separately) to help track and retrieve your sporting dogs in the field. You’ll see markers for each dog within tracking range, overlaid right on the navigator’s map screen. Directional cues help you easily follow and locate each dog you’re tracking.\"\n        image=\"https://res.garmin.com/en/products/010-02406-00/g/32245-DOG-f902f55f-438f-4fb7-90f9-a1e858293d45.jpg\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n      <pc-feature-card\n        data-translate\n        title=\"GO-ANYWHERE GUIDANCE\"\n        description=\"Spoken turn-by-turn directions if you choose to take Tread with you when you head back to civilisation and want on-road navigation.\"\n        image=\"https://res.garmin.com/en/products/010-02406-00/g/32245-12-4fdff974-8d94-49ec-95fc-4b70fc4b6867.jpg\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n      <pc-feature-card\n        data-translate\n        title=\"\"\n        description=\"\"\n        \n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n    </pc-column-layout>\n  </pc-layout-container>\n  <!-- end product feature section -->\n  <!-- start life style -->\n  <pc-life-style\n    data-translate\n    text=\"Onboard tools for the ultimate off-road experience.\"\n    text-size=\"2\"\n    text-align=\"right\"\n    text-position=\"middle\"\n    mobile-text-size=\"2\"\n    mobile-text-align=\"center\"\n    mobile-text-position=\"top\"\n    button-text=\"\"\n    link=\"\"\n    bg-image=\"https://res.garmin.com/en/products/010-02406-00/g/EMEA_32245-D-3-6347ab75-db14-4740-b95f-03dfd8f1c8be.jpg\"\n    mobile-bg-image=\"https://res.garmin.com/en/products/010-02406-00/g/EMEA_32245-M-3-edefaa85-c4d3-4d7c-b062-90004071c970.jpg\"\n    img-alt=\"\"\n    img-width=\"\"\n    theme=\"dark\"\n  />\n  <!-- end life style -->\n  <!-- start product feature section -->\n  <pc-layout-container>\n    <pc-column-layout\n      columns=\"3\"\n    >\n      <pc-feature-card\n        data-translate\n        title=\"LIVE WEATHER\"\n        description=\"Weather ahead? Pair Tread with your compatible smartphone, and download the Tread app to access live weather, animated radar and more<sup>3</sup>.\"\n        image=\"https://res.garmin.com/en/products/010-02406-00/g/32245-13-4327e97c-31b6-40b6-be49-b0dec36d4fa2.jpg\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n      <pc-feature-card\n        data-translate\n        title=\"TRAVELLING MUSIC\"\n        description=\"A built-in media player lets you easily control music loaded on your <a href='https://support.garmin.com/en-AU/?faq=lvfeD8uSkXAgxsnxdaAV0A&searchQuery'>compatible smartphone</a> on the sat nav’s display — and hear your tunes through your helmet or headset (sold separately) enabled with Bluetooth technology.\"\n        image=\"https://res.garmin.com/en/products/010-02406-00/g/32245-14-55c2871f-5ef7-4325-8b18-5cfa1a0b35c8.jpg\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n      <pc-feature-card\n        data-translate\n        title=\"ADD A WIRELESS CAMERA\"\n        description=\"What’s around you? Maximise visibility by pairing your Tread sat nav with a rugged <a href='https://www.garmin.com/en-AU/p/723564/'>BC™ 40</a> wireless camera with tube mount (sold separately). It easily mounts to your vehicle’s tube frame, flat panel or roll cage.\"\n        image=\"https://res.garmin.com/en/products/010-02406-00/g/32245-15-3da7d62e-e99a-4e27-a149-2cfa8ebf8672.jpg\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n    </pc-column-layout>\n    <pc-column-layout\n      columns=\"3\"\n    >\n      <pc-feature-card\n        data-translate\n        title=\"CONTROL YOUR ACCESSORIES\"\n        description=\"Pair Tread with the compatible <a href='https://www.garmin.com/en-AU/p/718849/'>Garmin PowerSwitch™</a> digital switch box (sold separately) to enable onscreen control for your side-by-side’s 12-volt electronics — light bars, differential locks and more.\"\n        image=\"https://res.garmin.com/en/products/010-02406-00/g/32245-16-de12c81b-44cb-430c-b0e6-f0f636c753e3.jpg\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n      <pc-feature-card\n        data-translate\n        title=\"POINTS OF INTEREST\"\n        description=\" Tread comes preloaded with iOverlander™ points of interest so you don’t need a mobile phone signal to route to the best-established, wild and dispersed campsites.\"\n        image=\"https://res.garmin.com/en/products/010-02406-00/g/32245-17-1d411dc6-d091-42b6-b6b3-f78f38160d86.jpg\"\n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n      <pc-feature-card\n        data-translate\n        title=\"\"\n        description=\"\"\n        \n        img-alt=\"\"\n        img-width=\"350px\"\n        content-align=\"left\"\n      />\n    </pc-column-layout>\n  </pc-layout-container>\n  <!-- end product feature section -->\n  <!-- start learn more section -->\n  <pc-layout-container\n    size=\"large\"\n    theme=\"neutral\"\n  >\n    <pc-column-layout\n      columns=\"2\"\n    >\n      <pc-learn-more\n        data-translate\n        title=\"Control your vehicle’s accessories with Garmin PowerSwitch™\"\n        description=\"\"\n        button-text=\"Learn more\"\n        link=\"https://www.garmin.com/en-AU/p/718849\"\n        bg-image=\"https://res.garmin.com/gdc/Product_Pages/LEARN-MORE-CARDS/POWERSWITCH-BOX-D.jpg\"\n        mobile-bg-image=\"https://res.garmin.com/gdc/Product_Pages/LEARN-MORE-CARDS/POWERSWITCH-BOX-M.jpg\"\n        img-alt=\"\"\n        theme=\"light\"\n      />\n      <pc-learn-more\n        data-translate\n        title=\"Communicate globally with inReach® satellite technology<sup>5</sup>.\"\n        description=\"\"\n        button-text=\"Learn more\"\n        link=\"https://www.garmin.com/en-AU/c/outdoor-recreation/satellite-communicators/?series=BRAND11460\"\n        bg-image=\"https://static.garmincdn.com/gdc/Product_Pages/LEARN-MORE-CARDS/inReachMini2-BOX-D.jpg\"\n        mobile-bg-image=\"https://static.garmincdn.com/gdc/Product_Pages/LEARN-MORE-CARDS/inReachMini2-BOX-M.jpg\"\n        img-alt=\"\"\n        theme=\"light\"\n      />\n    </pc-column-layout>\n  </pc-layout-container>\n  <!-- end learn more section -->\n  <!-- start disclaimer section -->\n  <pc-layout-container\n    size=\"\"\n    theme=\"neutral\"\n  >\n    <pc-disclaimer-block>\n      <pc-disclaimer-item\n        data-translate\n        counter=\"1\"\n        content=\"Group Tracking is available for riders using a Tread sat nav, and only riders with a Tread sat nav may be tracked. Connection to vehicle power is required for group tracking. The Garmin Group Ride Tracker functions within the VHF 169.4Mz frequency band to allow location sharing and position tracking. The use of radio equipment is subject to various national regulations and for usage outside of the European union, it is your responsibility to know and comply with applicable laws in jurisdictions where you plan to use this device. Within the European Union use of this product does not require a license.\"\n      />\n      <pc-disclaimer-item\n        data-translate\n        counter=\"2\"\n        content=\"Limitations may exist in map data for some road and trail route suggestions. Always defer to all posted road signs and conditions during vehicle operation. Garmin products using OpenStreetMap data are intended to be used only as supplementary travel aids.\"\n      />\n      <pc-disclaimer-item\n        data-translate\n        counter=\"3\"\n        content=\"To use this feature, you must have the <a href='https://www.garmin.com/en-AU/p/773341'>Tread app</a> downloaded on your compatible smartphone enabled with Bluetooth technology, have an active mobile phone signal connection and pair with the Tread sat nav.\"\n      />\n      <pc-disclaimer-item\n        data-translate\n        counter=\"4\"\n        content=\"Active satellite subscription required for the inReach device. Some jurisdictions regulate or prohibit the use of satellite communications devices. It is your responsibility to know and follow all applicable laws in the jurisdictions where the device is intended to be used.\"\n      />\n      <pc-disclaimer-item\n        data-translate\n        counter=\"5\"\n        content=\"Some jurisdictions regulate or prohibit the use of satellite communication devices. It is your responsibility to know and follow all applicable laws in the jurisdictions where the device is intended to be used.\"\n      />\n    </pc-disclaimer-block>\n    <pc-disclaimer-block>\n      <pc-disclaimer-item\n        data-translate\n        counter=\"\"\n        content=\"The Bluetooth word mark and logos are registered trademarks owned by Bluetooth SIG, Inc. and any use of such marks by Garmin is under licence. Wi-Fi® is a registered trademark of the Wi-Fi Alliance. Copyright ©2021 by iOverlander, LLC. Copyright ©2021 Ultimate Campgrounds - All rights reserved.\"\n      />\n    </pc-disclaimer-block>\n  </pc-layout-container>\n  <!-- end disclaimer section -->\n</div>"},"additionalTab":{"title":"FAQ","content":"<p><img src=\"\" /></p>\n<p><strong>Group Ride Range</strong></p>\n<p style=\"margin-bottom:1.5rem\">Range performance varies based on terrain and other line-of-sight limitations. Here are some examples of typical performance: <a href=\"\"></a></p>\n<h3>Upper figures - with antenna mounting kit (included with Tread®, not available for zūmo XT)​</h3>\n<p><img src=\"https://res.garmin.com/gdc/home-page/products/antenna_range.jpg\" style=\"width: 80%; margin-left: 10%;\" /></p>\n<h3>Lower figures - without antenna mounting kit​</h3><br />\n<p><strong>Do I need a license with the FCC in order to operate this radio?</strong></p>\n<p>No. The Tread product uses MURS (Multi-Use-Radio-Service) for radio technology, which means that you do not have to file a license with the FCC in order to use this product.</p>"},"mapsTab":{"title":"Maps","content":"<h2>Maps for New Regions</h2><p>Need maps for regions other than the ones that came on your device? See all maps that are compatible with your device.</p><form action=\"\" class=\"app__layout__flex-container\" id=\"form-compatible-maps\" name=\"form-compatible-maps\"><div class=\"app__layout__half\"><label for=\"js__regionSelect\">Where are you going?</label><select id=\"js__regionSelect\" class=\"app__select-box\" autocomplete=\"off\"><option value=\"\" selected=\"selected\">Select Continent</option><option value=\"Africa\">Africa</option><option value=\"Asia\">Asia</option><option value=\"Australia\">Australia Oceania</option><option value=\"Europe\">Europe</option><option value=\"Middle East\">Middle East</option><option value=\"North America\">North America</option><option value=\"South America\">South America</option><option value=\"all\">All</option></select></div><div class=\"app__layout__half\"><label for=\"js__activitySelect\">What type of maps do you need?</label><select id=\"js__activitySelect\" class=\"app__select-box\" autocomplete=\"off\"><option value=\"\" selected=\"selected\">Select Activity</option><option value=\"road\">On the Road</option><option value=\"marine\">On the Water</option><option value=\"topo\">On the Trail</option><option value=\"golf\">On the Green</option><option value=\"all\">All</option></select></div></form><div id=\"compatibleMaps\"></div><hr /><h2>Need a map update?</h2><p>Update the maps that came with your device for the best navigation experience.</p><p><a href=\"http://www.garmin.com/en-AU/maps/updates/\">Check for Updates</a></p>"},"inTheBoxTab":{"title":"In the Box","content":"<ul>\r\n\t<li>Tread&reg;</li>\r\n\t<li>VHF antenna</li>\r\n\t<li>Antenna mount kit</li>\r\n\t<li>Tube mount kit</li>\r\n\t<li>High-current power cable</li>\r\n\t<li>Trim ring kit (blue, black, red and green)</li>\r\n\t<li>USB cable</li>\r\n\t<li>Documentation</li>\r\n</ul>\r\n<img src=\"https://static.garmincdn.com/gdc/home-page/products/_Tread_HR_5000.png\" style=\"width: 80%; margin-left: 10%;\" />"},"accessoriesTab":{"title":"Accessories","content":null},"specsTab":{"title":"Specs","content":"<div><table>\n        <tbody>\n        <tr class=\"title\">\n            <td colspan=\"2\" style=\"background: #fff; color: #000; border: 1px solid #ddd; border-width: 0px 0px 1px 0px; padding: 0px;\">\n                <h3>General</h3>\n            </td>\n        </tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Dimension</th><td>5.8&Prime; W  x  3.5&Prime; H  x  1&Prime; D (8.8 x 14.8 x 2.4 cm)</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Weight</th><td>9.2 oz (262 g)</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\"> <a href=\"https://www.garmin.com/en-AU/legal/waterrating-definitions/\">Water rating</a> </th><td>IPX7</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Display Type</th><td>Multi-touch, glass, high brightness HD colour TFT with white backlight</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Display Size</th><td>4.8&Prime; W x 2.7&Prime; H (12.1 x 6.8 cm); 5.5&Prime; diagonal (13.9 cm)</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Display Resolution</th><td>1280 x 720 pixels</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Dual-Orientation Display</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Tube mount  (1.5\" - 2.375\")</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">1\" ball adapter with AMPS plate</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Drop rating</th><td>MIL-STD-810</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Battery type</th><td>Rechargeable lithium-ion</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Battery life</th><td>Up to 6 hours (up to 3.5 hours at 100% backlight)</td></tr></tbody></table><table>\n        <tbody>\n        <tr class=\"title\">\n            <td colspan=\"2\" style=\"background: #fff; color: #000; border: 1px solid #ddd; border-width: 0px 0px 1px 0px; padding: 0px;\">\n                <h3>Maps & Memory</h3>\n            </td>\n        </tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Preloaded street maps</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Data cards</th><td>microSD<sup>™</sup> card (not included)</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">3D Terrain</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Internal storage</th><td>32 GB</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Ability to add maps</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">External memory storage</th><td>Yes (256 GB max microSD™ card)</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Includes map updates</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Downloadable Satellite Imagery</th><td>yes (BirdsEye)</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\"> <a href=\"https://support.garmin.com/en-AU/?faq=jakwWTRa1F7at7zPW47Ui6\">BirdsEye Direct Satellite Imagery</a> </th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">TOPO map support</th><td class=\"yes\"></td></tr></tbody></table><table>\n        <tbody>\n        <tr class=\"title\">\n            <td colspan=\"2\" style=\"background: #fff; color: #000; border: 1px solid #ddd; border-width: 0px 0px 1px 0px; padding: 0px;\">\n                <h3>Sensors</h3>\n            </td>\n        </tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">GPS</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Galileo</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Barometric altimeter</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Compass</th><td class=\"yes\"></td></tr></tbody></table><table>\n        <tbody>\n        <tr class=\"title\">\n            <td colspan=\"2\" style=\"background: #fff; color: #000; border: 1px solid #ddd; border-width: 0px 0px 1px 0px; padding: 0px;\">\n                <h3>Outdoor Recreation</h3>\n            </td>\n        </tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Point-to-point navigation</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Preloaded topographical maps</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Navigate-a-track navigation</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Turn-by-turn trail navigation</th><td class=\"yes\"></td></tr></tbody></table><table>\n        <tbody>\n        <tr class=\"title\">\n            <td colspan=\"2\" style=\"background: #fff; color: #000; border: 1px solid #ddd; border-width: 0px 0px 1px 0px; padding: 0px;\">\n                <h3>Camera Features</h3>\n            </td>\n        </tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Backup camera compatible</th><td>yes (BC<sup>™</sup> 40 or BC<sup>™</sup> 50)</td></tr></tbody></table><table>\n        <tbody>\n        <tr class=\"title\">\n            <td colspan=\"2\" style=\"background: #fff; color: #000; border: 1px solid #ddd; border-width: 0px 0px 1px 0px; padding: 0px;\">\n                <h3>Navigation Features</h3>\n            </td>\n        </tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Garmin Real Directions™ landmark guidance</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">TripAdvisor® Traveler Ratings</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Route shaping through preferred cities/streets</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Includes traffic</th><td>Yes (with Garmin Tread app)</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Lane assist with junction view (displays junction signs)</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">BLUETOOTH® pairing</th><td>Yes, works with Bluetooth-compatible helmet, headset or smartphone. BLE capable.</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Speed limit indicator (displays speed limit for most major roads in Australia and New Zealand)</th><td class=\"yes\"></td></tr></tbody></table><table>\n        <tbody>\n        <tr class=\"title\">\n            <td colspan=\"2\" style=\"background: #fff; color: #000; border: 1px solid #ddd; border-width: 0px 0px 1px 0px; padding: 0px;\">\n                <h3>Advanced Features</h3>\n            </td>\n        </tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Smart notifications via app</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Wi-Fi® map and software updates</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Access to live services via app (traffic, weather, etc.)</th><td class=\"yes\"></td></tr></tbody></table><table>\n        <tbody>\n        <tr class=\"title\">\n            <td colspan=\"2\" style=\"background: #fff; color: #000; border: 1px solid #ddd; border-width: 0px 0px 1px 0px; padding: 0px;\">\n                <h3>Motorcycle Features</h3>\n            </td>\n        </tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Control music and media from smartphone or MP3 player</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Sunlight readable and weather resistant</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">GARMIN ADVENTUROUS ROUTING™</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\"><a href=\"https://support.garmin.com/en-AU/?faq=HvMyFz0QZ40m9QSYxDqR38&searchQuery=Creating%20and%20Loading%20Custom%20Maps%20to%20an%20Automotive%20Device%09\" target=\"_blank\">Custom map</a> support</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Wireless ANT+<sup>®</sup> technology</th><td class=\"yes\"></td></tr></tbody></table><table>\n        <tbody>\n        <tr class=\"title\">\n            <td colspan=\"2\" style=\"background: #fff; color: #000; border: 1px solid #ddd; border-width: 0px 0px 1px 0px; padding: 0px;\">\n                <h3>RV Features</h3>\n            </td>\n        </tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Easy Route Planner with GPX file sharing</th><td class=\"yes\"></td></tr></tbody></table><table>\n        <tbody>\n        <tr class=\"title\">\n            <td colspan=\"2\" style=\"background: #fff; color: #000; border: 1px solid #ddd; border-width: 0px 0px 1px 0px; padding: 0px;\">\n                <h3>Adventure Features</h3>\n            </td>\n        </tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Pair with compatible inReach® devices</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Pair with Garmin PowerSwitch™ digital switch box</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Compatible with external GPS antenna with mount</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Compatible with external inReach® satellite antenna</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Group Ride Mobile</th><td>Yes (with Garmin Tread app)</td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Track recorder (breadcrumbs)</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">iOverlander™ points of interest</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Ultimate Public Campgrounds</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Pitch and Roll gauges</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Birdseye Satellite Imagery (download via Wi-Fi)</th><td class=\"yes\"></td></tr><tr>\n            <th scope=\"row\" style=\"background-color: #fff; font-weight: 300;\">Glove-friendly touchscreen</th><td class=\"yes\"></td></tr></tbody></tabl