      "sidebar": {
        "additionalContainer": null,
        "quickLinks": {
          "content": "<p><a href=\"https://www.garmin.com/MarineMapUpdate/\">BlueChart Updates</a><br />\r\n<a href=\"https://www.garmin.com/en-US/cartography/marine/programs/\">Programs</a><br />\r\n<a href=\"https://supportforms.garmin.com/en-US/form/report-map-error/\">Report a Map Error</a><br />\r\n<a href=\"https://www.garmin.com/en-US/legal/bluechart-g3-disclaimer/\">Disclaimer</a></p>\r\n",
